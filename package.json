{
  "name": "text-pastry",
  "main": "./lib/text-pastry",
  "version": "0.4.4",
  "description": "Advanced pasting commands for multiple selection",
  "activationCommands": {
    "atom-workspace": [
      "text-pastry:paste-0-to-X",
